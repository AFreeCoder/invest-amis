                                "xAxis": {
                                    "type": "category",
                                    "boundaryGap": false,
                                    "data": "${x_data}"
                                },
                                "yAxis": [
                                    {
                                        "type": "value",
                                        "name": "收盘点位",
                                        "alignTicks": true,
                                        "position": "left"
                                    },
                                    {
                                        "type": "value",
                                        "name": "FED（股债性价比）",
                                        "alignTicks": true,
                                        "position": "right"
                                    }
                                ],
                                "series": [
                                    {
                                        "name": "收盘点位",
                                        "type": "line",
                                        "emphasis": {
                                            "focus": "series"
                                        },
                                        "data": "${y_cp}"
                                    },
                                    {
                                        "name": "股债性价比（FED）",
                                        "type": "line",
                                        "yAxisIndex": 1,
                                        "emphasis": {
                                            "focus": "series"
                                        },
                                        "data": "${y_fed}"
                                    }
                                ],
                                "dataZoom": [
                                    {
                                        "start": null,
                                        "end": null,
                                        "startValue": "${count - 1250}",
                                        "endValue": "${x_data|last}"
